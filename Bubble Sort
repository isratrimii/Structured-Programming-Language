#include<stdio.h>
int main()
{
    int arr[]={2, 6, 58, 47, 69, 10};
    int i, j, size=6;

    for(i=0; i<size-1; i++)
    {
        for(j=0;j<size-1; j++)
        {
            if(arr[j]>arr[j+1])
            {
                int temp=arr[j];
                arr[j]=arr[j+1];
                arr[j+1]=temp;
            }
        }
    }
    printf("\nBubble sort:\n");
for(i=0; i<size; i++){
        printf("%d", arr[i]);
    }
    printf("\n");


    return 0;
}
