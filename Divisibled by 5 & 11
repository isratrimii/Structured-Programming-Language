#include<stdio.h>
int main()
{
    int num;
    printf("Enter a number: ");
    scanf("%d", &num);

    if (num%5==0)
    {
        printf("%d is divisibled by 5 &11", num);
    }
    else if (num%11==0)
    {
       printf("%d is divisibled by 5 &11", num);

    }
    else
    {
         printf("%d is not divisibled by 5 &11", num);
    }
    return 0;
}
